{
  "name": "npo-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec babel-node src/server.js",
    "devtest": "cross-env NODE_ENV=test nodemon --watch src --exec babel-node src/server.test.js",
    "build": "babel src -d dist",
    "serve": "node dist/server.js",
    "prod:sit": "cross-env NODE_ENV=production node server.js",
    "prod:online": "cross-env NODE_ENV=production pm2 start server.js"
  },
  "dependencies": {
    "debug": "~2.6.8",
    "jsonwebtoken": "^8.3.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.3.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^3.1.0",
    "koa-passport": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.2",
    "koa-static": "^3.0.0",
    "koa-views": "^6.1.4",
    "mongoose": "^5.2.9",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "3.18.0",
    "nodemon": "^1.18.3",
    "pm2": "^3.0.3"
  }
}
